include <iostream>


int main()
{
    std::string stroka1, stroka2;
    std::string stroka3 = " ";
    std::cout << "Введите первую строку:" << std::endl;
    std::cin >> stroka1;
    std::cout << "Введите вторую строку:" << std::endl;
    std::cin >> stroka2;
    std::cout << stroka1 + stroka3 + stroka2 << std::endl;

    return 0;
}
